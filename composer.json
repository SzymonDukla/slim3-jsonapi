{
    "name": "carterzenk/slim3-json-api",
    "type": "library",
    "description": "Slim 3 Eloquent JSON API Transformer Package",
    "keywords": ["slim", "slim3", "eloquent", "json", "api", "jsonapi", "serializer", "transformer", "psr7", "response"],
    "license": "MIT",
    "authors": [
        {
            "name": "Carter Zenk",
            "email": "carterzenk@gmail.com"
        },
        {
            "name": "Nil Portugués Calderó",
            "email": "contact@nilportugues.com",
            "role": "Project Lead Developer"
        }
    ],
    "require": {
        "nilportugues/json-api": "^2.4",
        "symfony/psr-http-message-bridge": "~0.1",
        "nilportugues/serializer-eloquent": "~1.0",
        "nilportugues/eloquent-repository": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "friendsofphp/php-cs-fixer": "^1.10",
        "slim/slim": "^3.0",
        "illuminate/database": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "SzymonDukla\\Slim3\\JsonApi\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NilPortugues\\Tests\\" : "tests/NilPortugues/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
